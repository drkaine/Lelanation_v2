{
  "name": "lelanation-backend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "sync:data": "tsx src/scripts/syncData.ts",
    "riot:collect": "tsx src/scripts/runRiotMatchCollect.ts",
    "riot:worker": "tsx src/scripts/runRiotMatchCollectWorker.ts",
    "riot:backfill-ranks": "tsx src/scripts/runBackfillParticipantRanks.ts",
    "riot:backfill-roles": "tsx src/scripts/runBackfillParticipantRoles.ts",
    "riot:refresh-match-ranks": "tsx src/scripts/runRefreshMatchRanks.ts",
    "riot:cleanup-matches": "tsx src/scripts/runCleanupMatches.ts",
    "riot:discover-players": "tsx src/scripts/runRiotPlayerDiscovery.ts",
    "riot:discover-league-exp": "tsx src/scripts/runRiotLeagueExpDiscovery.ts",
    "riot:enrich": "tsx src/scripts/runRiotEnrich.ts",
    "copy:assets": "tsx src/scripts/copyStaticAssets.ts",
    "generate:excluded-items": "tsx src/scripts/generateExcludedItems.ts",
    "typecheck": "tsc --noEmit",
    "test:matchups": "tsx --test src/services/MatchupTierService.test.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/node-cron": "^3.0.11",
    "prisma": "^7.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.4.1",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-cron": "^4.2.1",
    "pg": "^8.18.0"
  }
}
